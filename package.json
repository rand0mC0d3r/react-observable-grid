  "name": "react-observable-grid",
  "version": "0.0.18",
  "description": "Â±Infinite grid of React components",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "private": false,
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.4.0",
    "copy-and-watch": "^0.1.6",
    "eslint-plugin-mui-unused-classes": "^1.0.3",
    "npm-check-updates": "12.0.2",
    "rollup": "^2.53.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-generate-declarations": "^1.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "chokidar-cli": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "serve": "13.0.2"
  },
  "peerDependencies": {
    "@material-ui/core": ">=4.12.3",
    "@material-ui/icons": ">=4.11.2",
    "react": ">=16.8.4"
  },
  "dependencies": {
    "fast-sort": "^3.1.1",
    "prop-types": "^15.7.2",
    "react-intersection-observer": "^8.32.5"
  },
  "files": [
    "LICENSE",
    "*.md",
    "dist"
  ],
  "keywords": [
    "reactjs",
    "react",
    "icon",
    "icons",
    "material-ui"
  ],
  "author": "rand0mC0d3r",
  "license": "MIT",
  "scripts": {
    "wipe": "rm -rf package-lock.json && rm -rf ./node_modules && cd sample && rm -rf package-lock.json && rm -rf ./node_modules",
    "dev": "concurrently \"npm run build.watch\" \"cd sample && export PORT=3339 && npx react-scripts start\"",
    "start": "serve -s build",
    "prebuild": "cd sample && npm install",
    "build": "rm -rf ./build && cd sample && npx react-scripts build && mv ./build ../",
    "build.lib": "rollup -c",
    "test": "react-scripts test",
    "clone.component": "clear && mkdir -p ./sample/src/components/ && cp -r src/* sample/src/components/ && echo 'Synchronized'",
    "copy.component": "npm run clone.component && copy-and-watch dist/* sample/src/components/ --watch",
    "tag": "npm run build.lib && git commit -am 'built next version' && npm version patch && git push && git push --tags && npm publish",
    "ship": "npm publish",
    "build.watch": "chokidar \"src/**/*.js\" -c \"npm run clone.component\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": "^7.20.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rand0mC0d3r/react-observable-grid.git"
  },
  "bugs": {
    "url": "https://github.com/rand0mC0d3r/react-observable-grid/issues"
  },
  "homepage": "https://github.com/rand0mC0d3r/react-observable-grid/blob/master/README.md",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
